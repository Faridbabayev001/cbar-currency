{
    "name": "faridbabayev/cbar-currency",
    "description": "PHP library to work with cbar.az currency rates",
    "keywords": ["cbar", "azerbaijan", "azn", "currency"],
    "minimum-stability": "stable",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Farid Babayev",
            "email": "faridbabayev98@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-simplexml": "*"
    },
    "autoload": {
        "psr-4": {
            "FaridBabayev\\CBARCurrency\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FaridBabayev\\CBARCurrency\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    }
}
